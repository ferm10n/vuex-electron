{
  "name": "@ferm10n/vuex-electron",
  "version": "1.1.0-beta.1",
  "description": "Vuex Electron",
  "keywords": [
    "vue",
    "vuex",
    "electron"
  ],
  "main": "./dist/index.js",
  "repository": "https://github.com/ferm10n/vuex-electron",
  "homepage": "https://github.com/ferm10n/vuex-electron",
  "bugs": "https://github.com/ferm10n/vuex-electron/issues",
  "authors": [
    "Andrew Emelianenko <emelianenko.web@gmail.com>",
    "John Sanders <jws121295@gmail.com>"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "rimraf dist && mkdirp dist",
    "build": "tsc",
    "watch": "yarn build --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "yarn lint && jest && bundlesize"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/**/*.js",
      "maxSize": "2 kB"
    }
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-minify": "^0.4.3",
    "bundlesize": "^0.18.1",
    "electron": "^7",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.6.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4",
    "vue": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "dependencies": {
    "deepmerge": "^2.1.1",
    "electron-store": "^8.0.0"
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "types.d.ts",
    "CHANGELOG.md",
    "LICENSE"
  ]
}
